<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BrowseMapFragment">
        <attr name="sygic_mapInteractionMode" format="enum">
            <enum name="none" value="0"/>
            <enum name="markersOnly" value="1"/>
            <enum name="full" value="2"/>
        </attr>

        <attr name="sygic_compassEnabled" format="boolean"/>
        <attr name="sygic_compassHideIfNorthUp" format="boolean"/>
        <attr name="sygic_positionLockFabEnabled" format="boolean"/>
        <attr name="sygic_zoomControlsEnabled" format="boolean"/>
        <attr name="sygic_cameraInitialLatitude" format="float"/> <!--todo: MS-4502-->
        <attr name="sygic_cameraInitialLongitude" format="float"/> <!--todo: MS-4502-->
        <attr name="sygic_cameraInitialZoom" format="float"/> <!--todo: MS-4502-->
    </declare-styleable>

</resources>