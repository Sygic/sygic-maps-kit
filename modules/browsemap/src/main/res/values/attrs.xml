<resources>

    <declare-styleable name="BrowseMapFragment">
        <attr name="sygic_map_selectionMode" format="enum">
            <enum name="none" value="0" />
            <enum name="markersOnly" value="1" />
            <enum name="full" value="2" />
        </attr>

        <attr name="sygic_compass_enabled" format="boolean" />
        <attr name="sygic_compass_hideIfNorthUp" format="boolean" />
        <attr name="sygic_positionOnMap_enabled" format="boolean" />
        <attr name="sygic_positionLockFab_enabled" format="boolean" />
        <attr name="sygic_zoomControls_enabled" format="boolean" />
    </declare-styleable>

</resources>