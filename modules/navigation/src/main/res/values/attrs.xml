<resources>

    <declare-styleable name="NavigationFragment">
        <attr name="sygic_navigation_distanceUnit" format="enum">
            <enum name="kilometers" value="0" />
            <enum name="miles_yards" value="1" />
            <enum name="miles_feets" value="2" />
        </attr>
        <attr name="sygic_navigation_previewMode" format="boolean" />
        <attr name="sygic_signpost_enabled" format="boolean" />
        <attr name="sygic_signpost_type" format="enum">
            <enum name="full" value="0" />
            <enum name="simplified" value="1" />
        </attr>
    </declare-styleable>

</resources>