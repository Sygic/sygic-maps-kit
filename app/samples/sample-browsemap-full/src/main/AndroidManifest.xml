<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.sygic.samples">

    <application
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher_browsemap_full"
        android:label="@string/browse_map_full"
        android:roundIcon="@mipmap/ic_launcher_browsemap_full_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sygic.Light.DarkActionBar"
        tools:ignore="GoogleAppIndexingWarning">

        <activity
            android:name=".BrowseMapFullActivity"
            android:label="@string/browse_map_full">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="@string/com_sygic_api_key"
            android:value="${sygicApiKey}" />

    </application>

</manifest>
